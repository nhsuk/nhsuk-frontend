{% set title = 'Card - primary card (with chevron)' %}
{% from 'nhsuk/components/card/macro.njk' import card %}
{% extends 'example.njk' %}

{% block content %}
  <div class="nhsuk-grid-row">
    <div class="nhsuk-grid-column-two-thirds">
      {{ card({
        "href": "#",
        "primary": "true",
