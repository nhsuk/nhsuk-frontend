<nav class="nhsuk-c-pagination{%- if params.classes %} {{ params.classes }}{% endif %}"{% for attribute, value in params.attributes %} {{attribute}}="{{value}}"{% endfor %} role="navigation" aria-label="Pagination">
  {% if params.previousURL and params.previousTitle %}
  <a href="{{ params.previousURL }}" class="nhsuk-c-pagination-prev">
    <span class="nhsuk-c-pagination-link">Previous</span>
    <span class="nhsuk-c-pagination-title">{{ params.previousTitle }}</span>
    {% include 'assets/icons/icon-arrow-left.svg' %}
  </a>
  {% endif %}
  {% if params.nextURL and params.nextTitle %}
  <a href="{{ params.nextURL }}" class="nhsuk-c-pagination-next">
    <span class="nhsuk-c-pagination-link">Next</span>
    <span class="nhsuk-c-pagination-title">{{ params.nextTitle }}</span>
    {% include 'assets/icons/icon-arrow-right.svg' %}
  </a>
  {% endif %}
</nav>
