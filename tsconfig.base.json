{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "esModuleInterop": true,
    "module": "Preserve",
    "moduleResolution": "Node",
    "noEmit": true,
    "paths": {
      "nhsuk-frontend": ["./packages/nhsuk-frontend/src/index.mjs"],
      "nhsuk-frontend/*": ["./packages/nhsuk-frontend/*"]
    },
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true
  },
  "exclude": ["**/node_modules", "**/dist"],
  "typeAcquisition": {
    "enable": false
  }
}
